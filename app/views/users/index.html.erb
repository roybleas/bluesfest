<h1>
	<%= pluralize(@usercount, 'User') %>
</h1>
<%= will_paginate %>

<ul class="users">
  <% @users.each do |user| %>
    <li>
      <%=   user.name %>
      ( <%= user.screen_name %> )
      <% if @admin && user.id != current_user.id %>
      <%= link_to "Delete", user, method: :delete,
                                  data: { confirm: "You sure?" } %>
      <% end %>
    </li>
  <% end %>
</ul>
<%= will_paginate %>
